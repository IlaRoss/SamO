    <!-- http://www.w3.org/2001/XMLSchema#gYear -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#gYear"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/NET/c4dm/event.owl#agent -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#agent"/>
    


    <!-- http://purl.org/NET/c4dm/event.owl#factor -->

    <owl:ObjectProperty rdf:about="http://purl.org/NET/c4dm/event.owl#factor"/>
    


    <!-- http://purl.org/ontology/mo/collaborated_with -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/collaborated_with">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/compilation_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/compilation_of">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/compiled -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/compiled">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/mo/compiler"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicArtist"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/compiler -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/compiler">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicArtist"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/genre -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/genre">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/MusicalExpression"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/MusicalManifestation"/>
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Performance"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Genre"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/instrument -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/instrument">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/event.owl#factor"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Instrument"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/performer -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/performer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/NET/c4dm/event.owl#agent"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/publication_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/publication_of">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/mo/published_as"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicalExpression"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/published_as -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/published_as">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/MusicalExpression"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/MusicalManifestation"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/recorded_as -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/recorded_as">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/mo/records"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/mo/records -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/mo/records">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Performance"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/EQed -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/EQed">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/EQed_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Indicates a process of manipulation using the EQing technique.</rdfs:comment>
        <rdfs:label>EQed</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/EQed_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/EQed_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Indicates a process of manipulation using the EQing technique.</rdfs:comment>
        <rdfs:label>EQed_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/chop_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/chop_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/chopped_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/ontology/samo/loop_of"/>
        <rdfs:comment>Indicates a process of manipulation using the chopping technique.</rdfs:comment>
        <rdfs:label>chop_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/chopped_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/chopped_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/ontology/samo/looped_into"/>
        <rdfs:comment>Indicates a process of manipulation using the chopping technique.</rdfs:comment>
        <rdfs:label>chopped_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/clip_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/clip_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/clipped_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Indicates a process of manipulation using the clipping technique.</rdfs:comment>
        <rdfs:label>clip_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/clipped_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/clipped_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Indicates a process of manipulation using the clipping technique.</rdfs:comment>
        <rdfs:label>clipped_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/detuned -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/detuned">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/detuned_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/ontology/samo/tuned"/>
        <rdfs:comment>Indicates a process of manipulation using the detuning technique.</rdfs:comment>
        <rdfs:label>detuned</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/detuned_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/detuned_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/ontology/samo/tuned_into"/>
        <rdfs:comment>Indicates a process of manipulation using the detuning technique.</rdfs:comment>
        <rdfs:label>detuned_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/filtered -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/filtered">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/filtered_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Indicates a process of manipulation using the filtering technique.</rdfs:comment>
        <rdfs:label>filtered</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/filtered_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/filtered_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Indicates a process of manipulation using the filtering technique.</rdfs:comment>
        <rdfs:label>filtered_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/has_elements -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/has_elements">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Relates a Track to any number of Elements, with a minimum of 1.</rdfs:comment>
        <rdfs:label>has_elements</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/has_sample -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/has_sample">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/used_in"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Relates a Track to an instance of a Sample.</rdfs:comment>
        <rdfs:label>has_sample</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/is_cover_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/is_cover_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:comment>Shortcut property to relate a Track with the track that covers it.</rdfs:comment>
        <rdfs:label>is_cover_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/is_sampled_in -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/is_sampled_in">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/samples"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:comment>Shortcut property to relate a Track with the track that samples it.</rdfs:comment>
        <rdfs:label>is_sampled_in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/loop_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/loop_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/looped_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Indicates a process of manipulation using the looping technique.</rdfs:comment>
        <rdfs:label>loop_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/looped_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/looped_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Indicates a process of manipulation using the looping technique.</rdfs:comment>
        <rdfs:label>looped_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/manipulated -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/manipulated">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/manipulated_with"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sampler"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:comment>Relates a Sampler to the Signal it manipulated.</rdfs:comment>
        <rdfs:label>manipulated</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/manipulated_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/manipulated_into">
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Relates an Element to a Sample, indicating a process of transformation from one class to the other through manipulation.</rdfs:comment>
        <rdfs:label>manipulated_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/manipulated_with -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/manipulated_with">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Signal"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sampler"/>
        <rdfs:comment>Relates a Signal to the Sampler used to manipulate it.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/manipulation_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/manipulation_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Relates a Sample to an Element, indicating a process of transformation from one class to the other through manipulation.</rdfs:comment>
        <rdfs:label>manipulation_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/part_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/part_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment>Functional property to indicate class relationships.</rdfs:comment>
        <rdfs:label>part_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/pitch_shift_of -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/pitch_shift_of">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulation_of"/>
        <owl:inverseOf rdf:resource="http://purl.org/ontology/samo/pitch_shifted_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:comment>Indicates a process of manipulation using the pitch shifting technique.</rdfs:comment>
        <rdfs:label>pitch_shift_of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/pitch_shifted_into -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/pitch_shifted_into">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/samo/manipulated_into"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/samo/Elements"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/samo/Sample"/>
        <rdfs:comment>Indicates a process of manipulation using the pitch shifting technique.</rdfs:comment>
        <rdfs:label>pitch_shifted_into</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/producer -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/producer">
        <rdfs:subPropertyOf rdf:resource="http://purl.org/ontology/mo/performer"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment>Extension of the Music Ontology performer sub-properties, used to relate the Sampling performance event to an agent. By default this agent should be the producer of the track, who is most likely to have created the sample in the act of production.</rdfs:comment>
        <rdfs:label>has producer</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/requires -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/requires">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Performance"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://purl.org/ontology/mo/Instrument"/>
                    <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment>Functional property to indicate class relationships.</rdfs:comment>
        <rdfs:label>requires</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/ontology/samo/samples -->

    <owl:ObjectProperty rdf:about="http://purl.org/ontology/samo/samples">
        <rdfs:domain rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/mo/Track"/>
        <rdfs:comment>Shortcut property to relate a Track with the track that it samples. For example.</rdfs:comment>
        <rdfs:label>samples</rdfs:label>
